<wr-page>
  <wr-content for="logo">
    <img src="assets/WebDefault/images/webratio-logo.png" style="max-width:200px; position:absolute; top: 20px; margin:0 auto;">
  </wr-content>
  <wr-content for="login">
    <wd-alert [messages]="valueOf('main/msg21/messages')" [localized]="true" type="warn"></wd-alert>
    <wd-card title="model:main/frm15">
      <wr-content for="title[frm15]">
        {{ labelOf('.') | wrLocalize }}
      </wr-content>
      <wr-content for="content">
        <form>
          <wd-input 
              [showClearIcon]="false" 
              floatLabel="never"
              context="model:main/frm15/fld44"
              [disableAutocompletion]="true">
          </wd-input>
          <wd-password 
              [showEyeButton]="true" 
              floatLabel="never" 
              context="model:main/frm15/fld45">
          </wd-password>
          <wd-checkboxes
              context="model:main/frm15/fld46" [showLabel]="false">
          </wd-checkboxes>
          <wd-button 
              context="model:main/frm15/oslte19" 
              color="primary">
          </wd-button>
        </form>
      </wr-content>
    </wd-card>
  </wr-content>
</wr-page>