<wr-page>
  <wr-content for="logo">
    <img src="assets/Logo_UNSa.jpg" style="max-width:200px; position:absolute; top: 20px; margin:0 auto;">
  </wr-content>
  <wr-content for="login">
    <wd-alert [messages]="valueOf('main/msg17/messages')" [localized]="true" type="warn"></wd-alert>
    <wd-card title="model:main/frm12">
      <wr-content for="title[frm12]">
        {{ 'Iniciar Session' | wrLocalize }}
      </wr-content>
      <wr-content for="content">
        <form>
          <wd-input 
              [showClearIcon]="false" 
              floatLabel="never"
              context="model:main/frm12/fld35"
              [disableAutocompletion]="true">
          </wd-input>
          <wd-password 
              [showEyeButton]="true" 
              floatLabel="never" 
              context="model:main/frm12/fld36">
          </wd-password>
          <wd-checkboxes
              context="model:main/frm12/fld37" [showLabel]="false">
          </wd-checkboxes>
          <wd-button 
              context="model:main/frm12/oslte13" 
              color="primary">
          </wd-button>
        </form>
      </wr-content>
    </wd-card>
  </wr-content>
</wr-page>